{
  "name": "UBCAutoLogin",
  "version": "3.1",
  "manifest_version": 2,
  "description": "UBC Logins made easy!",
  "icons": { "16": "icon.png",
			 "48": "icon.png",
			 "128": "icon.png" },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
	"default_popup": "popup.html"
  }
  "permissions": [
    "storage"
  ]
}